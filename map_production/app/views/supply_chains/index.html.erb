<p id="notice"><%= notice %></p>

<h1>Supply Chains</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @supply_chains.each do |supply_chain| %>
      <tr>
        <td><%= link_to 'Show', supply_chain %></td>
        <td><%= link_to 'Edit', edit_supply_chain_path(supply_chain) %></td>
        <td><%= link_to 'Destroy', supply_chain, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Supply Chain', new_supply_chain_path %>
